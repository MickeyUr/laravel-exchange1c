{
  "name": "alex8bits/laravel-exchange1c",
  "type": "library",
  "description": "Catalog Loader from 1c - CommerceML with laravel",
  "keywords": [
    "1c",
    "CommerceML",
    "Laravel"
  ],
  "homepage": "https://github.com/alex8bits/laravel-exchange1c",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Kartsev",
      "role": "Developer"
    },
    {
      "name": "Aleksey Balobanov",
      "email": "jalex21@mail.ru"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Bigperson\\LaravelExchange1C\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "laravel/framework": "^8.0|^9.0|^10.0|^11.0",
    "alex8bits/exchange1c": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5 || ^10.0",
    "orchestra/testbench": "v9.0",
    "mockery/mockery": "^1.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bigperson\\LaravelExchange1C\\Exchange1CServiceProvider"
      ]
    }
  }

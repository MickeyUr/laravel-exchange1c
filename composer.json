    "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "govdnocode/exchange1c": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "orchestra/testbench": "~4.0",
    "mockery/mockery": "^1.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Alexnsk83\\LaravelExchange1C\\Exchange1CServiceProvider"
      ]
    }
  }

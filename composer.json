{
  "name": "alexnsk83/laravel-exchange1c",
  "type": "library",
  "description": "Catalog Loader from 1c - CommerceML with laravel",
  "keywords": [
    "1c",
    "CommerceML",
    "Laravel"
  ],
  "homepage": "https://github.com/alexnsk83",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Kartsev",
      "role": "Developer"
    },
    {
      "name": "Aleksey Balobanov",
      "role": "Contributor"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Alexnsk83\\LaravelExchange1C\\": "src/"
    }
  },
  "autoload-dev": {
